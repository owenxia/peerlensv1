<%= simple_form_for @annotation do |f| %>
	<%= f.input :quote, label: '(Quote passed from page): ' %>
	<%= f.input :note, label: 'Note: ', placeholder: 'Enter your annotation here.' %>
	<%= f.input :notebook_id, collection: current_user.notebooks.all, selected: current_user.notebooks.first %>
	<%= f.button :submit %>
<% end %>