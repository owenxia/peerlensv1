<h1>This is the page where all of a user's annotations are shown</h1>

<% @annotations.each do |annotation| %>
	<div class="annotation_wrapper">
		<p class="quote"><strong>Quote:</strong><%= annotation.quote %></p>
		<p class="note"><strong>Note:</strong><%= annotation.note %></p>
		<p> <%= link_to 'Show Details', annotation %></p>
	</div>

<% end %>
